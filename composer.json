{
    "name": "tannhatcms/revise-operation",
    "description": "Backpack interface for venturecraft/revisionable",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Tabacitu",
            "email": "tabacitu@backpackforlaravel.com",
            "homepage": "https://backpackforlaravel.com"
        }
    ],
    "homepage": "https://github.com/laravel-backpack/revise-operation",
    "keywords": [
